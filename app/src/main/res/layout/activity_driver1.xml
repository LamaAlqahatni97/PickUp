<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1EEE1"
    tools:context=".Driver1"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="0dp"
            android:background="#F1EEE1"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/ic_home_black_24dp"/>

        <TextView
            android:id="@+id/textView95"
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:text="Car Information"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="#F70A0A0A"
            android:textSize="36sp" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="10dp"
            android:background="#F1EEE1"
            android:scaleType="centerCrop"
            app:srcCompat="@mipmap/ic_launcher" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="250dp">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="123dp"
            android:layout_height="55dp"
            android:text="Car type:"
            android:textColor="#0A0909"
            android:textSize="30sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="86dp"
            tools:layout_editor_absoluteY="282dp" />

        <TextView
            android:id="@+id/carplateD"
            android:layout_width="94dp"
            android:layout_height="48dp"
            android:layout_marginLeft="30dp"
            android:text="Toyota"
            android:textColor="#B40A0909"
            android:textSize="24sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="243dp"
            tools:layout_editor_absoluteY="282dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="30dp">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="130dp"
            android:layout_height="56dp"
            android:text="Plate no:"
            android:textColor="#0A0909"
            android:textSize="30sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="86dp"
            tools:layout_editor_absoluteY="368dp" />

        <TextView
            android:id="@+id/carTypeD"
            android:layout_width="94dp"
            android:layout_height="48dp"
            android:layout_marginLeft="30dp"
            android:text="1856"
            android:textColor="#B40A0909"
            android:textSize="24sp"
            tools:ignore="DuplicateIds,MissingConstraints"
            tools:layout_editor_absoluteX="252dp"
            tools:layout_editor_absoluteY="363dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="75dp">

        <Button
            android:id="@+id/goOnline"
            style="@android:style/Widget.Material.Light.Button.Borderless.Colored"
            android:layout_width="263dp"
            android:layout_height="70dp"
            android:background="#F2CF0F"
            android:text="Go Online"
            android:textAllCaps="true"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="#FFFFFF"
            android:textColorHighlight="#B5B50808"
            android:textColorLink="#F8F7F2F2"
            android:textSize="36sp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="532dp"
            tools:targetApi="lollipop" /> </LinearLayout>


</LinearLayout>